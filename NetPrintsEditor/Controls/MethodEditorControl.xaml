<UserControl x:Class="NetPrintsEditor.Controls.MethodEditorControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:NetPrintsEditor.Controls"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300" x:Name="methodEditorWindow">
    <AdornerDecorator>
        <Canvas x:Name="canvas">
            <Label Content="{Binding ElementName=methodEditorWindow, Path=Method.Name}" FontFamily="Lucida Sans Unicode" FontWeight="Bold" FontSize="20" />
        </Canvas>
    </AdornerDecorator>
</UserControl>
